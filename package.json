{
  "name": "phaser-world-explorer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "add": "^2.0.6",
    "fuzzy": "^0.1.3",
    "js-yaml": "^4.1.0",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-collapsible": "^2.8.4",
    "react-dom": "^17.0.2",
    "react-media": "^1.10.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3"
  },
  "devDependencies": {
    "husky": "^7.0.0",
    "lint-staged": "^11.2.0",
    "prettier": "2.4.1",
    "yarn": "^1.22.15"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "prepare": "husky install"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all",
    "not android 4.4.3-4.4.4"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "none"
  }
}
